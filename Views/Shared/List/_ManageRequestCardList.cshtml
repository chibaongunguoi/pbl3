@model List<ManageRequestCard>

<div class="card-body">
  <table class="table table-striped table-checkall">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Tên khóa học</th>
        <th scope="col">Mã học viên</th>
        <th scope="col">Tên học viên</th>
        <th scope="col">SĐT học viên</th>
        <th scope="col">Thao tác</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var item in Model)
      {
        @await Html.PartialAsync("Card/_ManageRequestCard", (object)item)
      }
    </tbody>
  </table>
  @if (Model.Count == 0)
  {
    <div class="text-center">Không có yêu cầu nào</div>
  }
</div>
<script>
  demo();
</script>