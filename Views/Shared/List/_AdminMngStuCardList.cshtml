@model List<AdminMngStuCard>

<div class="card-body table-responsive p-0">
    <table class="table table-striped table-checkall table-sm table-compact">
        <thead>
            <tr>                <th scope="col" style="width: 3%">#</th>
                <th scope="col" style="width: 6%">Mã số</th>
                <th scope="col" style="width: 20%">Họ và tên</th>
                <th scope="col" style="width: 6%">Giới tính</th>
                <th scope="col" style="width: 10%">Ngày sinh</th>
                <th scope="col" style="width: 15%">Số điện thoại</th>
                <th scope="col" style="width: 40%">Tác vụ</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var card in Model)
            {
                @await Html.PartialAsync("Card/_AdminMngStuCard", (object)card)
            }
        </tbody>
    </table>
</div>